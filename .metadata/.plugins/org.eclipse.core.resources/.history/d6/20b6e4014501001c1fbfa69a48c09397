import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		
		Scanner var = new Scanner(System.in);
		
		System.out.print("Digite o Valor em °C: ");
		double C = var.nextDouble();
		double F = 9 * C / 5 + 32;
		System.out.printf("Seu valor e F: %.2f%n", F);
		

	}

}
